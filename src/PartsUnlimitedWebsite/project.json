{
  "authors": [
    "Microsoft"
  ],
  "description": "Parts Unlimited Website",
  "version": "1.0.0-*",
  "compilationOptions": {
    "warningsAsErrors": true,
    "define": [ "DEMO" ]
  },
  "packExclude": "*.cmd",
  "webroot": "wwwroot",
  "dependencies": {
    "EntityFramework.Commands": "7.0.0-*",
    "EntityFramework.InMemory": "7.0.0-*",
    "EntityFramework.Relational": "7.0.0-*",
    "EntityFramework.SqlServer": "7.0.0-*",
    "Microsoft.AspNet.Antiforgery": "1.0.0-*",
    "Microsoft.AspNet.Authentication.Cookies": "1.0.0-*",
    "Microsoft.AspNet.Authentication.Facebook": "1.0.0-*",
    "Microsoft.AspNet.Authentication.Google": "1.0.0-*",
    "Microsoft.AspNet.Authentication.MicrosoftAccount": "1.0.0-*",
    "Microsoft.AspNet.Authentication.OpenIdConnect": "1.0.0-*",
    "Microsoft.AspNet.Authentication.Twitter": "1.0.0-*",
    "Microsoft.AspNet.Diagnostics": "1.0.0-*",
    "Microsoft.AspNet.Diagnostics.Entity": "7.0.0-*",
    "Microsoft.AspNet.Identity.EntityFramework": "3.0.0-*",
    "Microsoft.AspNet.Mvc": "6.0.0-*",
    "Microsoft.AspNet.Server.IIS": "1.0.0-*",
    "Microsoft.AspNet.Server.Kestrel": "1.0.0-*",
    "Microsoft.AspNet.Server.WebListener": "1.0.0-*",
    "Microsoft.AspNet.Session": "1.0.0-*",
    "Microsoft.AspNet.SignalR.Server": "3.0.0-*",
    "Microsoft.AspNet.StaticFiles": "1.0.0-*",
    "Microsoft.Framework.Caching.Abstractions": "1.0.0-*",
    "Microsoft.Framework.Caching.Memory": "1.0.0-*",
    "Microsoft.Framework.Configuration.Json": "1.0.0-*",
    "Microsoft.Framework.OptionsModel": "1.0.0-*",
    "Microsoft.Framework.Runtime.Abstractions": "1.0.0-*",
    "PartsUnlimited.Models": ""
  },
  "commands": {
    "WebListener": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.WebListener --server.urls http://localhost:5001",
    "Kestrel": "Microsoft.AspNet.Hosting --server Microsoft.AspNet.Server.Kestrel --server.urls http://localhost:5001",
    "run": "run server.urls=http://localhost:5001"
  },
  "scripts": {
    "postrestore": [ "npm install" ],
    "prepare": [ "grunt" ]
  },
  "exclude": [
    "wwwroot",
    "node_modules",
    "bower_components"
  ],
  "publishExclude": [
    "node_modules",
    "bower_components",
    "**.xproj",
    "**.user",
    "**.vspscc"
  ],
  "frameworks": {
    "dnx451": {
    },
    "dnxcore50": {
    }
  }
}
